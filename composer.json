{
    "name": "bwinkers/nugget",
    "description": "Validate complex JSON objects and process them on change.",
    "keywords": [
        "forms", "api", "business object", "activerules"
    ],
    "type": "library",
    "require": {
        "php" : ">=7.0",
        "mongodb/mongodb": "^1.1",
        "justinrainbow/json-schema": "^5.2",
        "nette/utils": "^2.4",
        "nette/forms": "^2.4"
    },
    "require-dev": {
        "tracy/tracy": "^2.4",
        "nette/tester": "^2.0",
        "squizlabs/php_codesniffer": "^2.0",
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Bwinkers\\Nugget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bwinkers\\Nugget\\Test\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Winkers",
            "email": "bwinkers@gmail.com"
        }
    ],
    "version": "0.1.0"
}
